{
  "name": "node-react-bp",
  "version": "1.0.0",
  "description": "A very simple NodeJS + React + Webpack boilerplate",
  "main": "src/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon build/server.js --watch build/server.js",
    "build": "webpack --config webpack.config.js && webpack --config webpack.server.config.js",
    "watch:client": "webpack --progress --colors --config webpack.config.js --watch",
    "watch:server": "webpack --progress --colors --config webpack.server.config.js --watch",
    "debug:server": "nodemon --debug build/server.js --watch build/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/davidvuong/node-react-bp.git"
  },
  "author": "David Vuong <david.vuong256@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel": "^6.5.2",
    "babel-core": "^6.7.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.1",
    "redux-devtools": "^3.1.1",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "bluebird": "^3.3.4",
    "express": "^4.13.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "redux": "^3.3.1",
    "superagent": "^1.8.0",
    "superagent-bluebird-promise": "^3.0.0"
  },
    "engines": {
    "node": "5.7.1"
  }
}
